    "@readme/variable": "^15.1.2",
    "@tippyjs/react": "^4.2.6",
    "cmd-ts": "^0.11.0",
    "ejs": "^3.1.8",
    "oas": "^20.4.0",
    "oas-normalize": "^8.3.1",
    "openapi-types": "^12.1.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "sass": "^1.57.1"
  }
